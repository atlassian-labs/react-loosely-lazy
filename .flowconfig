[declarations]
<PROJECT_ROOT>/node_modules/.*

[ignore]
<PROJECT_ROOT>/dist

[include]

[libs]

[lints]

[options]
exact_by_default=true
include_warnings=true
module.name_mapper='^react-loosely-lazy' ->'<PROJECT_ROOT>/packages/core/react-loosely-lazy/src'
module.name_mapper='^@react-loosely-lazy/manifest' ->'<PROJECT_ROOT>/packages/core/manifest/src'
server.max_workers=1

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
